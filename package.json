    "node": "10.13.0",
    "npm": "6.9.0"
  },
  "dependencies": {
    "@sentry/node": "^4.4.1",
    "bcrypt": "^3.0.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "pg": "^7.7.1",
    "pg-hstore": "^2.3.2",
    "pusher": "^2.2.0",
    "sequelize": "^4.41.2",
    "sparkpost": "^2.1.3",
    "universal-cookie": "^3.0.7",
    "validator": "^10.9.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0"
  }
